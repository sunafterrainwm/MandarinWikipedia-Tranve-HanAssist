{
  "name": "mediawiki_gadget-typescript-template_repository",
  "version": "0.0.0",
  "description": "A TypeScript template repository for MediaWiki Gadget. To allow anyone to create gadgets with TypeScript quickly.",
  "main": "dist/index",
  "repository": "https://github.com/Frederisk/MediaWikiGadget-TypeScript-TemplateRepository.git",
  "author": "Rowe Wilson Frederisk Holme <frederisk@outlook.com>",
  "license": "MIT",
  "scripts": {
    "check-type": "tsc",
    "compile": "babel --extensions .ts ./src -d ./dist",
    "lint": "eslint -c .eslintrc.yml --ext .ts ./src"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "types-mediawiki": "^1.2.0",
    "typescript": "^4.4.2"
  }
}
