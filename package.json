{
  "name": "mediawiki_gadget-typescript-template_repository",
  "version": "0.0.0",
  "description": "A TypeScript template repository for MediaWiki Gadget. To allow anyone to create gadgets with TypeScript quickly.",
  "main": "dist/index",
  "repository": "https://github.com/Frederisk/MediaWikiGadget-TypeScript-TemplateRepository.git",
  "author": "Rowe Wilson Frederisk Holme <frederisk@outlook.com>",
  "license": "MIT",
  "scripts": {
    "check-type": "tsc",
    "compile": "webpack --config webpack.config.js --mode development",
    "compile-prod": "webpack --config webpack.config.js --mode production",
    "babel": "babel --extensions .ts ./src --out-file ./dist/index.js",
    "lint": "eslint -c .eslintrc.yml --ext .ts ./src"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-loader": "^8.2.5",
    "core-js": "^3.22.7",
    "eslint": "^7.32.0",
    "types-mediawiki": "^1.2.0",
    "typescript": "^4.4.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "jshint": "^2.13.4"
  }
}
